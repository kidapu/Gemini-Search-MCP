{
  "name": "gemini-search-mcp",
  "owner": {
    "name": "kidapu",
    "email": "kidapu@users.noreply.github.com"
  },
  "metadata": {
    "description": "Gemini 2.5 Flash + Google Search Grounding MCP server for Claude Code",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "gemini-search",
      "source": {
        "source": "url",
        "url": "https://github.com/kidapu/Gemini-Search-MCP.git"
      },
      "description": "AI agent powered by Gemini 2.5 Flash with Google Search grounding. Useful for finding the latest information, troubleshooting errors, researching topics, and discussing ideas.",
      "version": "0.0.1",
      "author": {
        "name": "kidapu",
        "email": "kidapu@users.noreply.github.com"
      },
      "category": "AI Tools",
      "tags": ["gemini", "google", "search", "ai", "mcp"],
      "strict": false,
      "mcpServers": {
        "gemini-search": {
          "command": "npx",
          "args": ["-y", "@kidapu/gemini-search-mcp"],
          "env": {
            "GEMINI_SEARCH_MCP_API_KEY": "${GEMINI_SEARCH_MCP_API_KEY}"
          }
        }
      }
    }
  ]
}
